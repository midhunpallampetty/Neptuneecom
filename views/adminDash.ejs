      <script>
    // Function to create and update the line chart
    function updateChart(data, labels, chartTitle) {
  var ctx = document.getElementById('salesChart').getContext('2d');
  var salesChart = new Chart(ctx, {
    type: 'line',
    data: {
      labels: labels,
      datasets: [{
        label: chartTitle,
        data: data,
        borderColor: '#15ec01',
        borderWidth: 4,
        
        fill: false,
        pointStyle: 'circle',
      pointRadius: 10,
      pointHoverRadius: 10,
      tension: 0.1
      }]
    },
    options: {
      responsive: true,
      plugins: {
        title: {
          display: true,
          text: 'Point Style: ' + 'pointStyle', // You can customize this part
        },
      },
  
      scales: {
        x: {
          type: 'category',
          labels: labels,
        },
        y: {
          beginAtZero: true
        }
      }
    }
  });
}

    
