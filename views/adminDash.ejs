            label: chartTitle,
            data: data,
            borderColor: '#15ec01',
            borderColor: '#9c1165',
            borderWidth: 4,
            fill: false,
            pointStyle: 'circle',
      pointRadius: 10,
      pointHoverRadius: 10,
      tension: 0.1
            
          }]
        },
        options: {
          scales: {
            x: {
              type: 'category',
              labels: labels,
                
            },
            y: {
              beginAtZero: true
            }
          },
 
        }
      });
    }
    function updateChart3(data, labels, chartTitle) {
      var ctx = document.getElementById('salesChart3').getContext('2d');
      const config = {
  type: 'line',
  data: {
    labels: labels,
    datasets: [{
      label: chartTitle,
      data: data,
      borderColor: '#2c2f91',
      borderWidth: 4,
      fill: false,
      pointStyle: 'circle',
      pointRadius: 10,
      pointHoverRadius: 10,
      tension: 0.1
    }]
  },
  options: {
    responsive: true,
    scales: {
      x: {
        type: 'category',
        labels: labels,
      },
      y: {
        beginAtZero: true
      }
    },

    plugins: {
      customCanvasBackgroundColor: {
        color: 'lightGreen',
      }
    }
  }
};

var salesChart = new Chart(ctx, config);

    }
