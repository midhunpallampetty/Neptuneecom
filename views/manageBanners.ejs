            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status: all</option>
                                <option>Active only</option>
                                <option>Disabled</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <% banners.forEach(banner => { %>
                <div class="card mb-3">
                  <img class="card-img-top" src="<%= banner.image %>" alt="Card image cap" style="height: 40vh; width: 100%;">

                  <div class="card-body">
                    <h5 class="card-title"><%= banner.slogan %></h5>
                    <p class="card-text">Start Date: <%= new Date(banner.startTime).toLocaleDateString('en-US', { day: 'numeric', month: 'long', year: 'numeric' }) %> </p>
                    <p class="card-text">End Date: <%= new Date(banner.endTime).toLocaleDateString('en-US', { day: 'numeric', month: 'long', year: 'numeric' }) %> </p>
                    <p>Status: <%= banner.isActive ? 'Enabled' : 'Disabled' %></p>
                    <a href="/admin/disable-banner/<%= banner._id %>" class="btn <%= banner.isActive ? 'btn-danger' : 'btn-success' %>">
                      <% if (banner.isActive) { %>
                        Disable
                      <% } else { %>
                        Enable
                      <% } %>
                    </a>
                    
                    
        </li>
    
                   
                    
